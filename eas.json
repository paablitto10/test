{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
	    "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest",
        "credentialsSource": "local"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2",
        "credentialsSource": "local"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "YOUR_APPLE_ASC_ID"
      }
    }
  }
}
